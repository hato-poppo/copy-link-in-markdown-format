{
    "manifest_version": 3,
    "name": "Copy Link In Markdown Format",
    "version": "0.2.0",
    "description": "記事タイトルとそのURLをマークダウン形式のリンクとしてコピーする為の拡張機能",

    "background": {
      "service_worker": "event.js"
    },
  
    "permissions": [
      "activeTab",
      "contextMenus"
    ],
    "icons": {
      "16": "public/icon-16.png",
      "48": "public/icon-48.png"
    }
  }
